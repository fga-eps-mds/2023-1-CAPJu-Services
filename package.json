{
  "name": "user",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "nodemon --legacy-watch src/server.js",
    "test": "jest --config=jest.config.mjs --detectOpenHandles --coverage",
    "lint": "eslint src",
    "migration": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "shred": "npx sequelize-cli db:migrate:undo:all",
    "prettify": "prettier --write src",
    "check-format": "prettier --check src",
    "format": " npx prettier --write src --ignore-unknown . && prettier --check src &&  npx eslint --fix src --ext .js"
  },
  "dependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "argon2": "^0.31.2",
    "babel-jest": "^29.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.0",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "request-ip": "^3.3.0",
    "sequelize": "^6.31.1"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "jest-sonar": "^0.2.16",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  }
}
