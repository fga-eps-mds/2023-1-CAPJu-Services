{
  "name": "processmanagement",
  "version": "1.0.0",
  "main": "server.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "nodemon --legacy-watch src/server.js",
    "test": " jest --config=jest.config.mjs --detectOpenHandles --coverage",
    "lint": "eslint src",
    "migration": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
    "shred": "npx sequelize-cli db:migrate:undo:all",
    "prettify": "npx prettier --write src",
    "check-format": "npx prettier --check src",
    "format": " npx prettier --write src --ignore-unknown . && npx prettier --check src && npx eslint --fix src --ext .js"
  },
  "dependencies": {
    "@aternus/csv-to-xlsx": "^2.1.2",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "axios": "^1.4.0",
    "babel-jest": "^29.5.0",
    "cors": "^2.8.5",
    "csv2xlsx": "^1.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.1",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "jest": "^29.5.0",
    "jsonwebtoken": "^9.0.0",
    "jspdf": "^2.5.1",
    "jspdf-autotable": "^3.6.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "node-xlsx": "^0.23.0",
    "pg": "^8.11.0",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "sequelize": "^6.31.1",
    "xlsx-js-style": "^1.2.0",
    "xlsx-to-csv": "^1.0.2"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "jest-sonar": "^0.2.16",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8"
  }
}
